  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "lint": "eslint -c .eslintrc src",
    "lint:dev": "nodemon --exec \"yarn lint\"",
    "lint:fix": "eslint -c .eslintrc --fix src",
    "start:server": "nodemon --exec 'npm run start:build:server' --watch src/server --watch ../common -e js,jsx --ignore node_modules/ --ignore ../common/node_modules",
    "start:client": "node scripts/start.js",
    "start:build:server": "babel-node src/server -i node_modules/",
    "start": "npm-run-all -p start:client start:server",
    "start:styleguide": "styleguidist server",
    "deploy": "pm2 deploy production",
    "deploy:setup": "pm2 deploy production setup",
    "deploy:server": "pm2 deploy production",
    "staging": "pm2 deploy staging",
    "staging:server": "npm run build:server && npm run test && npm run release && pm2 deploy staging",
    "predeploy": "npm run test && npm run build && npm run build:server && npm run release",
    "prestaging": "npm run test && npm run build:staging && npm run build:server && npm run release",
    "predeploy:server": "npm run build:server && npm run test",
    "build:staging:old": "node scripts/staging.js",
    "build:staging": "AMBIENTE=staging NODE_ENV=production webpack --config config/webpack.config.staging.js --progress --colors",
    "build:local": "NODE_ENV=production webpack --config config/webpack.config.staging.js --progress --colors",
    "build": "AMBIENTE=producao NODE_ENV=production webpack --config config/webpack.config.prod.js --progress --colors",
    "build:server": "node scripts/prebuild-server.js && BABEL_ENV=server babel src/server -d server --ignore node_modules/ && cp src/server/package.json server/",
    "test": "echo 'jest tests/client/cronograma/row.test.js'",
    "release": "yarn version --new-version patch",
    "preversion": "npm run test",
    "version": "git add --all",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "postversion": "git push origin",
    "postinstall": "yarn link common",
    "postupgrade": "yarn postinstall",
    "runjob": "node scripts/runjob.js"
  },
  "repository": {
    "url": "git@disem5.intranet.bb.com.br:infor/modelo.git"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
